{
  "configurations": {
    "run": {
      "adapter": "debugpy",
      "default": true,
      "configuration": {
        "request": "launch",
        "type": "python",
        "cwd": "${workspaceRoot}",
        "stopOnEntry": true,
        "program": "${workspaceRoot}/src/_PROJECTNAME_/__main__.py",
        "args": []
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userHandled": ""
        }
      }
    }
  }
}
